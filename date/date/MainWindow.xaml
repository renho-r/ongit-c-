<Window x:Class="date.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:date"
        mc:Ignorable="d"
        Title="MainWindow" Height="530" Width="500">
    <Grid Margin="0,0,0,0" Height="100">
        <WrapPanel HorizontalAlignment="Left" Height="50" VerticalAlignment="Top" Width="480" Margin="10, 10">
            <TextBox x:Name="textBox" Height="20" TextWrapping="Wrap" Width="350"/>
            <Button x:Name="button" Content="Button" Width="100" Click="showTime"/>
        </WrapPanel>
        <TextBox x:Name="textBoxShow" HorizontalAlignment="Left" Height="20" TextWrapping="Wrap" Width="350" Margin="10, 10"/>
    </Grid>
</Window>
